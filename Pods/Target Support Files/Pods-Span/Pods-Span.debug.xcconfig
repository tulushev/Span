FRAMEWORK_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Reveal-iOS-SDK/Reveal-Framework-iOS-1.6.2"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/GlitchKit" "${PODS_ROOT}/Headers/Public/Reveal-iOS-SDK"
LIBRARY_SEARCH_PATHS = $(inherited) "$CONFIGURATION_BUILD_DIR/GlitchKit"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/GlitchKit" -isystem "${PODS_ROOT}/Headers/Public/Reveal-iOS-SDK"
OTHER_LDFLAGS = $(inherited) -ObjC -l"GlitchKit" -l"z" -framework "CFNetwork" -framework "CoreGraphics" -framework "QuartzCore" -framework "Reveal"
PODS_ROOT = ${SRCROOT}/Pods
